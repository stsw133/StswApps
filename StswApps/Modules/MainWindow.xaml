<se:StswWindow x:Class="StswApps.MainWindow"
               xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
               xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
               xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
               xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
               xmlns:local="clr-namespace:StswApps"
               xmlns:se="clr-namespace:StswExpress;assembly=StswExpress"
               mc:Ignorable="d" Height="500" Width="750" WindowStartupLocation="CenterScreen"
               Title="{x:Static se:StswFn.AppNameAndVersion}" Icon="/Resources/icon.ico">
    <se:StswContentDialog Identifier="MainDialog">
        <local:MainContext/>
    </se:StswContentDialog>
</se:StswWindow>
